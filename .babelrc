{
    "presets":[
        ["@babel/preset-env", {
            "debug": false,
            "modules": false,
            "useBuiltIns": false
        }],
        "@babel/preset-react"
    ],
    "plugins": [
        "syntax-dynamic-import",
        "@babel/plugin-transform-runtime",
        "transform-async-to-generator"
    ],
    "env": {
        "production": {
          "presets": ["react-optimize"]
        }
    }
}